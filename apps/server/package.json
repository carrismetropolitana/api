{
	"name": "@carrismetropolitana/api-server",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"main": "index.js",
	"scripts": {
		"build": "npm run clean && tsc && resolve-tspaths && cp -r ./src/assets ./dist/assets",
		"clean": "rm -rf dist",
		"dev": "tsx watch ./src/index.ts",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"start": "npm run build && node dist/index.js"
	},
	"dependencies": {
		"@carrismetropolitana/api-services": "*",
		"@dotenv-run/cli": "1.3.6",
		"fastify": "5.0.0",
		"jwt-decode": "4.0.0",
		"luxon": "3.5.0",
		"protobufjs": "7.4.0",
		"redis": "4.7.0"
	},
	"devDependencies": {
		"@carrismetropolitana/eslint": "20241014.1938.30",
		"@types/luxon": "3.4.2",
		"@types/node": "22.7.5",
		"resolve-tspaths": "0.8.22",
		"tsx": "4.19.1",
		"typescript": "5.6.3"
	}
}
